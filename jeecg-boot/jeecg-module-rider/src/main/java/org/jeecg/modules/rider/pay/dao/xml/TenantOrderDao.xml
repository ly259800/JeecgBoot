<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.jeecg.modules.rider.pay.dao.TenantOrderMapper">

    <resultMap type="org.jeecg.modules.rider.pay.entity.SysTenantOrderEntity" id="tenantOrderMap">
        <result property="id" column="id"/>
        <result property="tenantCode" column="tenant_code"/>
        <result property="outTradeNo" column="out_trade_no"/>
        <result property="orderState" column="order_state"/>
        <result property="orderType" column="order_type"/>
        <result property="currentUserNum" column="current_user_num"/>
        <result property="expirationDate" column="expiration_date"/>
        <result property="expandNum" column="expand_num"/>
        <result property="expandUnitPrice" column="expand_unit_price"/>
        <result property="renewYear" column="renew_year"/>
        <result property="renewUnitPrice" column="renew_unit_price"/>
        <result property="renewDateStart" column="renew_date_start"/>
        <result property="renewDateEnd" column="renew_date_end"/>
        <result property="totalAmount" column="total_amount"/>
        <result property="actualAmount" column="actual_amount"/>
        <result property="description" column="description"/>
        <result property="payId" column="pay_id"/>
        <result property="payer" column="payer"/>
        <result property="successTime" column="success_time"/>
        <result property="paymentMethod" column="payment_method"/>
        <result property="userId" column="user_id"/>
        <result property="username" column="username"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <select id="getMaxOutTradeNo" resultType="java.lang.String">
        select max(out_trade_no) from sys_tenant_order ${ew.customSqlSegment}
    </select>

    <select id="pageQuery" resultMap="tenantOrderMap">
        select id,tenant_code,out_trade_no,order_state,order_type,current_user_num,expiration_date,expand_num,expand_unit_price,renew_year,renew_unit_price,
               renew_date_start,renew_date_end,total_amount,actual_amount,description,pay_id,payer,success_time,cancel_id,canceler,cancel_time,payment_method,
               user_id,username,create_time,update_time
        from sys_tenant_order
        ${ew.customSqlSegment}
    </select>

</mapper>
